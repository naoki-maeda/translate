<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <title>Login Mode</title>
    <link rel="stylesheet"
          href="${request.static_url('translate:html/css/bootstrap.min.css') }"/>
    <link rel="stylesheet"
          href="${request.static_url('translate:html/css/login.css') }"/>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src='/html/js/login.js' type="text/javascript" ></script>

  </head>
  <body>
      <p class="form-title">Sign In</p>
      <div id="form">
          <p>Email</p>
          <p class="email"><input type="email" id='email'></p>
          <p>Password</p>
          <p class="password"><input type="password" id='password'></p>
          <p class="submit">
          <input type="submit" id='submit' class="btn btn-primary" value="送信">
          </p>
      </div>
  </body>
</html>
